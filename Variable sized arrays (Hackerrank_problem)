#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {

    int n, q;
    cin >> n >> q;
    vector<int> a[n];
    for (int i = 0; i < n; i++){
        int x;
        cin >> x;
        for (int j = 0; j < x; j++){
            int input;
            cin >> input;
            a[i].push_back(input);
        }
    }

    int ij[q][2];   
    for (int i = 0; i < q; i++){
        for (int j = 0; j < 2; j++){
            cin >> ij[i][j];
        }
    }
    
    for (int l = 0; l < q; l++){
        int y;
        int i, j;
        i = ij[l][0];
        j = ij[l][1];
        y = (a[i])[j];
        cout << y << endl;
    }

    return 0;
}
